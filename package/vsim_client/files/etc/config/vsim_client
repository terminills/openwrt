config vsim_client 'main'
	option api_url 'http://localhost:5000/api'
	option api_key ''
	option device_id ''
	option heartbeat_interval '60'
	option command_poll_interval '30'
	option api_timeout '30'
	option log_level 'INFO'
	option log_file '/var/log/vsim_client.log'
	option max_retries '3'
	option retry_delay '5'